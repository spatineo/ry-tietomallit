  <element name="LandUseFeatureCollection" type="lud-core:LandUseFeatureCollectionType" substitutionGroup="gml:AbstractGML">
    <annotation>
      <documentation>
        A generic FeatureCollection to be used as a container for related Land Use features
      </documentation>
    </annotation>
  </element>
  <complexType name="LandUseFeatureCollectionType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence minOccurs="0" maxOccurs="unbounded">
          <element name="featureMember">
            <complexType>
              <complexContent>
                <extension base="gml:AbstractFeatureMemberType">
                  <sequence>
                    <element ref="gml:AbstractFeature"/>
                  </sequence>
                </extension>
              </complexContent>
            </complexType>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  
